server {
	server_name trilby2.ourapps.work www.trilby2.ourapps.work;
	access_log /var/log/nginx/trilby.log vhost;
	location / {
                try_files $uri @nodejs;
        }
	location @nodejs {
                include /etc/nginx/proxy_params;
                proxy_pass http://127.0.0.1:8080;
        }
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/trilby2.ourapps.work/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/trilby2.ourapps.work/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}
